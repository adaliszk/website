{
    "private": true,
    "version": "0.5.0",
    "name": "@adaliszk.io/content",
    "description": "The content for the adaliszk.io sites in MDX and JSON format.",
    "types": "./dist/vite.d.ts",
    "main": "./dist/vite.js",
    "type": "module",
    "scripts": {
        "format": "biome format --write .",
        "lint": "biome lint ./*.ts",
        "build": "tsup vite.ts --clean --format esm --dts --external lightningcss --out-dir ./dist"
    },
    "dependencies": {
        "@adaliszk/qwik-static-content": "workspace:*"
    },
    "exports": {
        "./projects": {
            "import": "./projects/index.ts"
        },
        ".": {
            "types": "./dist/vite.d.ts",
            "import": "./dist/vite.js"
        }
    }
}
